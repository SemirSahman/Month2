package ba.bitcamp.week8.day3.vjezbe.ArrayLists;

public class Main {

	public static void main(String[] args) {
		
		Book[] books = new Book[5];

		books[0] = new Book("Book1", "Autor1", 2014);
		books[1] = new Book("Book2");
		books[2] = new Book("Book3", 1990);
		books[3] = new Book("Book4");
		books[4] = new Book("Book5", 1887);
		
		
		for(Book i : books){
			System.out.println(i);
		}
	}

}
