public class Main {

	public static void main(String[] args) {
		
		
		Line l = new Line(10);
		
		l.drawOnConsole();
		System.out.println(l.getArea());
		l.getCircumference();
		
		
		Rectangle r = new Rectangle(4, 3);
		
		r.drawOnConsole();
		r.getArea();
		r.getCircumference();
		

	}

}
