package ba.bitcamp.week8.day3.vjezbe;

public class ShiftingAnArray {
	
	public static char[] shiftArray(char[] arr){
		
		for(int i = 0;i<arr.length;i++){
			arr[i+1] = arr[i];
		}
		return arr;
		
	}

	public static void main(String[] args) {
		char[] arr = {'A', 'B', 'C', 'D'};
		shiftArray(arr);

	}

}
